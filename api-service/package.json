{
  "name": "api-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev:old": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "dev": "nodemon server.ts",
    "db:migrate": "TZ=UTC node_modules/.bin/sequelize db:migrate",
    "db:undo": "TZ=UTC && node_modules/.bin/sequelize db:migrate:undo",
    "db:rollback": "TZ=UTC && node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@types/sequelize": "^4.28.15",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.1.4",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.9.1",
    "mysql2": "^3.3.5",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.0",
    "sequelize-cli": "^6.6.1",
    "sequelize-typescript": "^2.1.5",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.3",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  }
}
