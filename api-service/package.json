{
  "name": "api-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "db:migrate": "TZ=UTC node_modules/.bin/sequelize db:migrate",
    "db:undo": "TZ=UTC && node_modules/.bin/sequelize db:migrate:undo",
    "db:rollback": "TZ=UTC && node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.1.4",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^3.3.5",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.0",
    "sequelize-cli": "^6.6.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.3",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  }
}
